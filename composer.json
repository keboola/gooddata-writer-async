{
	"name": "keboola/gooddata-writer",
	"type": "symfony-bundle",
	"description": "Keboola GoodData Writer",
	"keywords": [],
	"authors": [
		{
			"name": "Jakub MatÄ›jka",
			"email": "jakub@keboola.com"
		}
	],
	"require": {
		"php": ">=5.3.5"
	},
	"require-dev": {
		"keboola/syrup": "~1.2.0",
		"phpunit/phpunit": "~4.0"
	},
	"scripts": {
		"post-install-cmd": [
			"Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
			"php vendor/keboola/syrup/app/console cache:clear --env=test"
		],
		"post-update-cmd": [
			"Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
			"php vendor/keboola/syrup/app/console cache:clear --env=test"
		]
	},
	"minimum-stability": "stable",
	"autoload": {
		"psr-0": { "Keboola\\GoodDataWriter": "" }
	},
	"target-dir": "Keboola/GoodDataWriter",
	"extra": {
		"symfony-app-dir": "vendor/keboola/syrup/app",
		"symfony-web-dir": "vendor/keboola/syrup/web",
		"incenteev-parameters": {
			"keep-outdated": true,
			"file": "vendor/keboola/syrup/app/config/parameters.yml",
			"dist-file": "parameters.yml"
		}
	}
}
