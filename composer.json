{
	"name": "keboola/gooddata-writer",
	"type": "symfony-bundle",
	"description": "Keboola GoodData Writer",
	"keywords": [],
	"authors": [
		{
			"name": "Jakub MatÄ›jka",
			"email": "jakub@keboola.com"
		}
	],
	"repositories": [
		{
			"type": "vcs",
			"url": "git@bitbucket.org:keboola/syrup-development-bundle.git"
		},
		{
			"type": "package",
			"package": {
				"name": "gooddata/gooddata-cl",
				"version": "1.2.67",
				"dist": {
					"url": "http://developer.gooddata.com/downloads/cl-tool/gooddata-cli.zip",
					"type": "zip"
				}
			}
		}
	],
	"require": {
		"php": ">=5.3.5",
		"emagister/zendframework1": "1.*",
		"gooddata/gooddata-cl": "1.2.*"
	},
	"require-dev": {
		"syrup/component-bundle": "1.0.*",
		"syrup/development-bundle": ">=1.0.8",
		"phpunit/phpunit": "3.7.*"
	},
	"scripts": {
		"post-install-cmd": [
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
			"cp /www/syrup-development-bundle/app/config/parameters.yml  vendor/syrup/development-bundle/app/config/"
		],
		"post-update-cmd": [
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache"
		]
	},
	"minimum-stability": "alpha",
	"autoload": {
		"psr-0": { "Keboola\\GoodDataWriter": "" }
	},
	"target-dir": "Keboola/GoodDataWriter",
	"extra": {
		"symfony-app-dir": "vendor/syrup/development-bundle/app",
		"symfony-web-dir": "vendor/syrup/development-bundle/web"
	}
}
